{
    "name": "freelance-delivery-service",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "lint": "yarn run eslint",
        "lint:fix": "yarn run eslint --fix",
        "test": "jest"
    },
    "dependencies": {
        "@auth0/nextjs-auth0": "^1.4.0",
        "@chakra-ui/icons": "^1.0.13",
        "@chakra-ui/react": "^1.6.1",
        "@emotion/react": "^11.1.5",
        "@emotion/styled": "^11.3.0",
        "@fontsource/montserrat": "^4.3.0",
        "@fontsource/ubuntu": "^4.3.0",
        "@fontsource/yanone-kaffeesatz": "^4.4.5",
        "@types/axios": "^0.14.0",
        "@types/lodash": "^4.14.170",
        "@types/react-datepicker": "^3.1.8",
        "@types/recompose": "^0.30.7",
        "axios": "^0.21.1",
        "chakra-ui-autocomplete": "^1.3.0",
        "dotenv-expand": "^5.1.0",
        "emotion-theming": "^11.0.0",
        "framer-motion": "^4",
        "lodash": "^4.17.21",
        "next": "10.1.3",
        "react": "17.0.2",
        "react-datepicker": "^3.8.0",
        "react-dom": "17.0.2",
        "react-google-maps": "^9.4.5",
        "react-google-places-autocomplete": "^3.3.1",
        "react-hook-form": "^7.0.7",
        "react-icons": "^4.2.0",
        "react-query": "^3.13.6",
        "recompose": "^0.30.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.15",
        "@storybook/addon-actions": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/react": "^6.2.9",
        "@testing-library/dom": "^7.30.3",
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/react": "^11.2.6",
        "@testing-library/react-hooks": "^5.1.1",
        "@types/react": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^26.6.3",
        "jest-dom": "^4.0.0",
        "prettier": "^2.2.1"
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts,tsx}",
            "!**/*.d.ts",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/.next/"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    }
}
