    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "prettier": "^2.2.1"
  },
      "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts,tsx}",
            "!**/*.d.ts",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/.next/"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    }
}
